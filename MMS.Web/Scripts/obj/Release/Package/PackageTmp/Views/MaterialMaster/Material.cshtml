                IsLotNumberTracking: $('#IsLotNumberTracking').is(":checked"),
                IsIssueStrictly: $('#IsIssueStrictly').is(":checked"),
                IsPrimaryPackage: $('#IsPrimaryPackage').is(":checked"),
                IsSecondaryPackage: $('#IsSecondaryPackage').is(":checked"),
            },
            success: function (data) {
                if (data.MaterialMasterId != "" || data.MaterialMasterId != 0) {
                    alert('Updated Successfully.');
                    location.href = "/MaterialMaster/Material";
                    return false;
                }
                else {
                    alert('Updation failed.');
                    location.href = "/MaterialMaster/Material";
                    return false;
                }

            },
            error: function (ex) {
                alert('Already Exist in the database.');
            }
        });
    }
</script>

<h2 class="content-title">Material</h2>

<div style="width: 100%; float: left;">

    <div id="tabVehicleWatchList">
        @Html.Action("MaterialMasterGrid", "MaterialMaster")
    </div>

</div>
