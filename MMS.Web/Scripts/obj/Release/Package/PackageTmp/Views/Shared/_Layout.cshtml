    <style>
        .drilldown a {
            color: #fff;
            text-decoration: none;
            display: block;
            padding: 10px;
            position: relative;
        }

            .drilldown a:hover {
                color: #fff;
                text-decoration: none;
            }

            .drilldown a span {
                position: absolute;
                right: 20px;
            }

        .drilldown ul {
            margin: 0;
            padding: 0;
            list-style: none;
        }

        .drilldown li {
            padding: 0px;
            border-bottom: 1px solid #333;
        }

        .drilldown {
            overflow: hidden;
            width: 100%;
            padding: 0;
            margin: 20px auto;
            background-color: #32549e;
            -webkit-transform: translate3d(0, 0, 0);
            transform: translate3d(0, 0, 0);
        }

        .drilldown-sub {
            display: none;
        }

        .drilldown-back {
            font-weight: bold;
        }

        .drilldown-root li a i {
            margin-right: 10px;
        }
    </style>

    <script>
      //  $('.drilldown').drilldown();
        $(document).ready(function () {
           // $('.drilldown').drilldown();
            //Material Name List Popup
            $(document).on("click", ".mater_listers li", function () {
                $(this).siblings().removeClass("back_color");
                $(this).addClass("back_color");
                var optionText = $(this).html();
                var optionValue = $(this).attr("id");
                $(".ui-autocomplete-input").val(optionText);
                $("#MaterialName").find('option').removeAttr("selected");
                $("#MaterialName").val(optionValue).find("option[value=" + optionValue + "]").attr('selected', true);
            });
            //Toogle click
            $(document).on("click", ".lefterr_icon", function () {
                if ($(this).hasClass("collapsed")) {
                    $(this).removeClass("collapsed");
                    var _cont_height = $(window).height() - 79;
                    var _cont_width = $(window).width() - 250;
                    $(".content-wrapper").css({ "height": _cont_height, "width": _cont_width });
                    $(".left-side-bar").animate({
                        width: ["toggle", "swing"]
                    });
                    $(".direction").toggleClass("toggle"), 300;
                }
                else {
                    $(this).addClass("collapsed");
                    var winWid = $(window).width();
                    var winHt = $(window).height();
                    $(".content-wrapper").css({ "height": winHt, "width": winWid });
                    $(".left-side-bar").animate({
                        width: ["toggle", "swing"]
                    });
                    $(".direction").toggleClass("toggle"), 300;
                }
            });
        });
    </script>
