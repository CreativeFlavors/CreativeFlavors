@model MMS.Web.Models.GradeMasterModel.GradeMasterModel 
@using MMS.Repository.Managers
@using MMS.Core.Entities
@{
   
} 

<div class="right-form-section clearfix">
    <form role="form" class="form-inline">
        <div class="container-fluid">
            <div style="margin-top: 20px;" class="row">
                <div class="col-md-4 col-sm-4 col-lg-4 col-xs-12 no-padding"></div>
                <div class="col-md-4 col-sm-4 col-lg-4 col-xs-12 no-padding">
                    <div class="field-section">
                        <div class="form-group">
                            <div class="col-md-5 col-sm-5 col-lg-5 col-xs-12">
                                <label class="form-label" for="text">Code NO</label>
                            </div>
                            <div class="col-md-7 col-sm-7 col-lg-7 col-xs-12">
                               @Html.HiddenFor(x => x.GradeMasterID)
                               @Html.TextBoxFor(m => m.CodeNo, new { @class = "form-control text-field" })
                            </div>
                        </div>
                    </div>
                    <div class="field-section">
                        <div class="form-group">
                            <div class="col-md-5 col-sm-5 col-lg-5 col-xs-12">
                                <label class="form-label" for="text">Category</label>
                            </div>
                            <div class="col-md-7 col-sm-7 col-lg-7 col-xs-12">
                                @Html.TextBoxFor(m => m.Category, new { @class = "form-control text-field" })
                            </div>
                        </div>
                    </div>
                    <div class="field-section">
                        <div class="form-group">
                            <div class="col-md-5 col-sm-5 col-lg-5 col-xs-12">
                                <label class="form-label" for="text">Designation</label>
                            </div>
                            <div class="col-md-7 col-sm-7 col-lg-7 col-xs-12">
                                @Html.TextBoxFor(m => m.Designation, new { @class = "form-control text-field" })
                            </div>
                        </div>
                    </div>
                    <div class="field-section">
                        <div class="form-group">
                            <div class="col-md-5 col-sm-5 col-lg-5 col-xs-12">
                                <label class="form-label" for="text">Grade</label>
                            </div>
                            <div class="col-md-7 col-sm-7 col-lg-7 col-xs-12">
                                @Html.TextBoxFor(m => m.Grade, new { @class = "form-control text-field" })
                            </div>
                        </div>
                    </div>
                    <div class="field-section">
                        <div class="form-group">
                            <div class="col-md-5 col-sm-5 col-lg-5 col-xs-12">
                                <label class="form-label" for="text">Efficiency</label>
                            </div>
                            <div class="col-md-7 col-sm-7 col-lg-7 col-xs-12">
                                @Html.TextBoxFor(m => m.Efficiency, new { @class = "form-control text-field" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="form-footer">
            <div class="row">
                <div class="col-md-12 col-sm-12 col-lg-12 col-xs-12 text-center">
                    <input type="button" onclick="Cancel()" class="btn btn-default btn-style" title="Cancel" value="Cancel">
                    @if (Model != null && Model.GradeMasterID != 0)
                    {

                        <input type="button" onclick="Update()" class="btn btn-default btn-style" id="GradeMasterID" title="Update" value="Update">
                    }
                    else
                    {
                        <input type="button" onclick="Save()" class="btn btn-default btn-style" title="Save" value="Save">
                    }
                </div>
            </div>
        </div>

    </form>
</div>

