    List<BuyerMaster> buyerMasterlist = new List<BuyerMaster>();
    BuyerManager buyerManager = new BuyerManager();
    buyerMasterlist = buyerManager.Get();
    Model.BuyerMasterList = buyerMasterlist;
    
    
}
<script>
   
    $('#Nextbtn').show();
    $('#Childtbl').hide();
    $('#Parenttbl').show();
    $('#save_Updatebtn').hide();
    // A $( document ).ready() block.
    $(document).ready(function () {
        $('#Nextbtn').show();
        $('#Childtbl').hide();
        $('#Parenttbl').show();
        $('#save_Updatebtn').hide();
    });
    $("#Nextbtn").click(function () {

        $('#Nextbtn').hide();
        $('#Childtbl').show();
        $('#Parenttbl').hide();
        $('#save_Updatebtn').show();

    });
    $("#Back").click(function () {

        $('#Nextbtn').show();
        $('#Childtbl').hide();
        $('#Parenttbl').show();
        $('#save_Updatebtn').hide();

    });
    //$(function () {
    $("#AgentName").change(function () {       
        $.ajax({
            type: 'POST',
            url: ' @Url.Action("AgentModelDetail", "BuyerMater")',
            data: { agentName: $('select#AgentName option:selected').text() },
