@model MMS.Web.Models.OrginMasterModel.OrginMasterModel
@{
    int id = Model.OriginMasterId;
    Session["OriginMasterId"] = id;
}

<div class="right-form-section clearfix">
    <form role="form" class="form-inline">
        <div class="container-fluid">
            <div style="margin-top: 20px;" class="row">
                <div class="col-md-3 col-sm-3 col-lg-3 col-xs-12 no-padding"> </div>
                <div class="col-md-6 col-sm-6 col-lg-6 col-xs-12 no-padding">
                    <div class="field-section">
                        <div class="form-group">
                            <div class="col-md-5 col-sm-5 col-lg-5 col-xs-12">
                                <label class="form-label" for="text">Code</label>
                            </div>
                            <div class="col-md-7 col-sm-7 col-lg-7 col-xs-12">
                              
                                @Html.HiddenFor(x => x.OriginMasterId)
                                @Html.TextBoxFor(m => m.Code, new { @class = "form-control text-field",@readonly=true })
                            </div>
                        </div>
                    </div>
                    <div class="field-section">
                        <div class="form-group">
                            <div class="col-md-5 col-sm-5 col-lg-5 col-xs-12">
                                <label class="form-label" for="text">OriginName</label>
                            </div>
                            <div class="col-md-7 col-sm-7 col-lg-7 col-xs-12">
                               
                                @Html.TextBoxFor(m => m.OriginName, new { @class = "form-control text-field" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="form-footer">
            <div class="row">
                <div class="col-md-12 col-sm-12 col-lg-12 col-xs-12 text-center">
                    <input type="button" onclick="Cancel()" class="btn btn-default btn-style" title="Back" value="Back">
                    @if (Model != null && Model.OriginMasterId != 0)
                    {

                        <input type="button" onclick="Update()" class="btn btn-default btn-style" id="OriginMasterId" title="Update" value="Update">
                    }
                    else
                    {
                        <input type="button" onclick="Save()" class="btn btn-default btn-style" title="Save" value="Save">
                    }
                </div>
            </div>
        </div>

    </form>
</div>

