<script>

    $(document).ready(function () {

        $(".numeric").bind("keypress", function (e) {
            var keyCode = e.which ? e.which : e.keyCode
            var ret = ((keyCode >= 48 && keyCode <= 57) || specialKeys.indexOf(keyCode) != -1);
            $(".error").css("display", ret ? "none" : "inline");
            return ret;
        });
        $(".numeric").bind("paste", function (e) {
            return false;
        });
        $(".numeric").bind("drop", function (e) {
            return false;
        });

    });

    $(document).on('change', '#SizeRangeMasterId', function () {
        $.ajax({
            type: "GET",
            dataType: "JSON",
            url: '@Url.Action("GetSizeRange", "MaterialOpeningStock")',
            data: { SizeRangeMasterId: $(this).val() },
            success: function (data) {
                var count = 1;
                $("#sizetable tbody").html("");
                for (var i = 0; i <= data.length - 1; i++) {
                    $("#sizetable tbody").append("<tr><td>" + data[i].Size + "</td><td><input type='text' id='qtyId" + count + "' value='0' class='form-control text-field' /></td><td></td></tr>");
                    count++;
                }
            }
        });
    });

