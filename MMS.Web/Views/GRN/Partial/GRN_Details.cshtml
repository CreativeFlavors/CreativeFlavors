@model MMS.Web.Models.GRNModel.GRNModel
@using MMS.Repository.Managers

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/css1/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css1/custom.css" rel="stylesheet" />
    <link href="~/Content/css1/responsive.css" rel="stylesheet" />
    <link href="~/Content/css1/variables.css" rel="stylesheet" />
    <link href="~/Content/css1/dropdown.css" rel="stylesheet" />

    <style>
        table, th, td {
            border: 1px solid #C9C9C9;
            border-collapse: collapse;
            text-align: center;
        }

        td {
            padding: 1px;
        }

        th {
            padding: 22px 25px;
            font-size: 14px;
            font-family: 'Poppins';
            color: #292b2c;
        }

        .salesOrder td input {
            border: navajowhite;
            width: 162px;
            text-align: center;
        }

        .salesOrder {
            margin-left: 11px;
            margin-top: 7%;
        }

        .add-remove button {
            min-width: 31px;
            height: 30px;
            background: #7f54b3;
            font-size: 14px;
            border: navajowhite;
            color: #fff;
        }
    </style>
</head>

<body>
    <div class="main-page">
        <section class="inner-section">
            <div class="edit-customer">
                <div class="back-button" onclick="cancel()"><img src="~/Content/assets/images/choose-plan.png" alt="back arrow"></div>
                <h2 class=" font-20">Goods Received Note</h2>
            </div>

            <div class="edit-section">
                <div class="d-flex justify-content-start">
                    <h3 class="font-20">GRN Information</h3>
                    <div id="alert" style="padding: 7px 19px;">

                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-1 col-md-5 col-sm-12">
                        <div class="input-part">
                            <label class="font-14" for="GrnHeaderId">GRN No</label>
                            @Html.TextBoxFor(m => m.GrnHeaderId, new { @class = "form-control text-field", @readonly = "true", style = "background-color: #eee !important;text-align: center;", id = "GrnHeaderId" })
                        </div>
                    </div>
                    <div class="col-lg-2 col-md-5 col-sm-12">
                        <div class="input-part">
                            <label class="font-14" for="SupplierId">Supplier</label>
                            @Html.DropDownListFor(m => m.SupplierId, MMS.Web.ExtensionMethod.HtmlHelper.SupplierName(), new { @class = "select-style text-field " })
                        </div>
                    </div>

                    <div class="col-lg-2 col-md-5 col-sm-12">
                        <div class="input-part">
                            <label class="font-14" for="#PoHeaderId">Purchase order No</label>
                            @Html.DropDownListFor(m => m.PoHeaderId, MMS.Web.ExtensionMethod.HtmlHelper.PONo(), new { @class = "select-style text-field " })
                        </div>
                    </div>
                    <div class="col-lg-2 col-md-5 col-sm-12">
                        <div class="input-part">
                            <label class="font-14" for="RefInvoiceDate">Ref Invoice Number</label>
                            @Html.TextBoxFor(m => m.RefInvoiceNumber, new { @class = "form-control text-field", @id = "RefInvoiceNumber", placeholder= "Enter the Ref Invoice Number" })
                        </div>
                    </div> 
                    <div class="col-lg-2 col-md-5 col-sm-12">
                        <div class="input-part">
                            <label class="font-14" for="RefInvoiceDate">Ref Invoice Date</label>
                            @Html.TextBoxFor(m => m.RefInvoiceDate, new { @class = "form-control text-field", @id = "RefInvoiceDate", type="date" })
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-5 col-sm-12 d-flex justify-content-center">
                        <div class="checkbox-wrapper-55" style=" margin-top: 6%;">
                            <label class="rocker rocker-small">
                                <input type="checkbox" id="currencyToggle">
                                <span class="switch-left" value="USD">USD</span>
                                <span class="switch-right" value="ZAR">ZAR</span>
                            </label>
                        </div>
                    </div>

                    <table style="width:100%" class="salesOrder mt-4">
                        <thead>
                            <tr>
                                <th>Product Name</th>
                                <th>Product Code</th>
                                <th>Batch Code</th>
                                <th>Expiry Date</th>
                                <th>Qty</th>
                                <th>UOM</th>
                                <th>Unit Price</th>
                                <th>Disc %</th>
                                <th>Discount Amount</th>
                                <th>Sub Total</th>
                                <th>Tax Rate</th>
                                <th>Tax Amount</th>
                                <th>Total Price</th>
                                <th>Add to Grid</th>
                            </tr>
                        </thead>
                        <tbody id="polist">
                        </tbody>
                    </table>
                    <div class="mt-2">
                        <div>
                            @{Html.RenderPartial("~/Views/GRN/Partial/GRNList.cshtml");}
                        </div>
                    </div>
                    <div>
                        <div class="d-flex justify-content-between">

                            <div class="form-group col-3 mt-3">
                                <label for="exampleFormControlTextarea1">Shipment Details</label>
                                @Html.TextAreaFor(m => m.ShipmentDetails, new { @class = "form-control mt-2", rows = "3", placeholder = "Comments ....." })
                            </div>
                            <div class="form-group col-3 mt-3">
                                <label for="additionalcommends">Notes</label>
                                @Html.TextAreaFor(m => m.Notes, new { @class = "form-control mt-2", rows = "3", placeholder = "Additional Comments ....." })
                            </div>
                            <div class=" form-group col-3 mt-3">
                                <div class="form-group row ">
                                    <label for="inputEmail3" class="col-sm-4 col-form-label" id="price">Price</label>
                                    <div class="col-sm-6">
                                        @Html.TextBoxFor(m => m.Total_Price, new { @class = "form-control", disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group row mt-3">
                                    <label for="inputEmail3" class="col-sm-4 col-form-label">Discount</label>
                                    <div class="col-sm-6">
                                        @Html.TextBoxFor(m => m.Total_discountval, new { @class = "form-control", disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group row mt-3">
                                    <label for="subtotal" class="col-sm-4 col-form-label">Sub Total</label>
                                    <div class="col-sm-6">
                                        @Html.TextBoxFor(m => m.Total_Subtotal, new { @class = "form-control", disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group row mt-3">
                                    <label for="inputEmail3" class="col-sm-4 col-form-label">Total Tax</label>
                                    <div class="col-sm-6">
                                        @Html.TextBoxFor(m => m.Total_TaxValue, new { @class = "form-control", disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group row mt-3">
                                    <label for="inputEmail3" class="col-sm-4 col-form-label">Grand Total</label>
                                    <div class="col-sm-6">
                                        @Html.TextBoxFor(m => m.Total_Grandtotal, new { @class = "form-control", disabled = "disabled" })
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="inner-section-button">
                            <button class="fill-color font-15" onclick="confirmGRN()">
                                Confirm GRN
                            </button>
                            <button class="border-color font-15" onclick="cancel()">
                                CANCEL
                            </button>
                        </div>
                    </div>
</section>
    </div>



    <!-- page-contents end -->
    <script src="~/Content/js/custom.js"></script>
    <script src="~/Content/js/dropdown.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

    <script src="~/Content/js/bootstrap.bundle.min.js"></script>
    <script>
        new TomSelect("#SupplierId", {
            create: false,
            sortField: {
                field: "text",
                direction: "asc"
            }
        });
        $(window).on("load", function () {
            $(".report-inner-section").addClass("transection");
            $("#GRN").attr('checked', true);
        });
        function confirmGRN() {
            var SupplierId = $("#SupplierId").val();
            var PoHeaderId = $("#PoHeaderId").val();
            var RefInvoiceNumber = $("#RefInvoiceNumber").val();
            var RefInvoiceDate = $("#RefInvoiceDate").val();
            var currencyOption = $('#currencyToggle').is(':checked') ? 'USD' : 'ZAR';
            var Total_TaxValue = $("#Total_TaxValue").val();
            var Total_Subtotal = $("#Total_Subtotal").val();
            var Total_Grandtotal = $("#Total_Grandtotal").val();
            var Total_discountval = $("#Total_discountval").val();
            var Total_Price = $("#Total_Price").val();
            var Notes = $("#Notes").val();
            var ShipmentDetails = $("#ShipmentDetails").val();
            var formdata = new FormData();
            formdata.append("SupplierId", SupplierId);
            formdata.append("PoHeaderId", PoHeaderId);
            formdata.append("RefInvoiceNumber", RefInvoiceNumber);
            formdata.append("RefInvoiceDate", RefInvoiceDate);
            formdata.append("Total_discountval", Total_discountval);
            formdata.append("currencyOption", currencyOption);
            formdata.append("Total_Subtotal", Total_Subtotal);
            formdata.append("Total_Grandtotal", Total_Grandtotal);
            formdata.append("Total_TaxValue", Total_TaxValue);
            formdata.append("Total_Price", Total_Price);
            formdata.append("Notes", Notes);
            formdata.append("ShipmentDetails", ShipmentDetails);
            $.ajax({
                type: 'POST',
                url: '/GRN/ConfirmGRN',
                contentType: false,
                processData: false,
                dataType: 'html',
                data: formdata,
                success: function (values) {
                    var data = JSON.parse(values);
                    if (data.AlertMessage === "Confirm Order") {
                        Swal.fire({
                            title: 'Success!!',
                            text: 'Your Order Confirmed Successfully!',
                            icon: 'success',
                            timer: 2000,
                            timerProgressBar: true,
                            showConfirmButton: false
                        });
                        setTimeout(function () {
                            window.location.href = "/GRN/GRNMaster";
                            return false;
                        }, 2000);
                    }
                    else if (data.AlertMessage === "Not Existed") {
                        alert("Before Confirm you must add the Product");
                    }
                }

            });
        }
        function Delete(arg) {
            $.ajax({
                type: "DELETE",
                url: '/GRN/SODelete',
                data: { SOId: arg },
                success: function (values) {
                    if (values.AlertMessage === "Success") {
                        alert("Deleted Successfully.");
                        $("#grncart").empty();
                        $.each(values.model.grnCarlist, function (i, item) {
                            $("#grncart").append(`
                                 <tr>
                                     <td id="taxper">${i}</td>
                                     <td id="Grandtotal1">${item.productname}</td>
                                     <td id="taxper">${item.unitprice}</td>
                                     <td id="TaxValue1">${item.quantity}</td>
                                     <td id="Grandtotal1">${item.discountvalue}</td>
                                     <td id="taxper">${item.subtotal}</td>
                                     <td id="TaxValue1">${item.taxvalue}</td>
                                     <td id="Grandtotal1">${item.grandtotal}</td>
                                     <td class="veh-4" style="width:12%;">
                                         <span class="table_icons">
                                             <img src="/Content/assets/images/delete-icon.png" onclick="Delete('${item.grncartid}')" />
                                         </span>
                                     </td>
                                 </tr>
                             `);
                        });
                    } else {
                        alert("Failed to delete.");
                    }
                },
                error: function (xhr, status, error) {
                    alert("An error occurred: " + xhr.responseText);
                }
            });
        }
    </script>
    <script>
        $(document).ready(function () {

            $('#PoHeaderId').change(function () {
                var PoHeaderId = $(this).val();
                $.ajax({
                    url: '/GRN/GetPODetails',
                    type: 'GET',
                    data: { POno: PoHeaderId },
                    success: function (response) {
                        $("#polist").empty();
                        $("#Total_Price").val("");
                        $("#Total_Subtotal").val("");
                        $("#Total_discountval").val("");
                        $("#Total_TaxValue").val("");
                        $("#Total_Grandtotal").val("");
                        $("#Total_Price").val(response.Total_Price);
                        $("#Total_Subtotal").val(response.Total_Subtotal);
                        $("#Total_discountval").val(response.Total_discountval);
                        $("#Total_TaxValue").val(response.Total_TaxValue);
                        $("#Total_Grandtotal").val(response.Total_Grandtotal);
                        $.each(response.polist, function (i, item) {
                            $("#polist").append(`
                                                                              <tr>
                                                                          <td style="background-color: #e9ecef;    width: 19%;text-align: left; id="productname">${item.ProductName}</td>
                                                                          <td style="background-color: #e9ecef" id="Productcode">${item.Productcode}</td>
                                                                          <td><input type="text" id="BatchCode" class="form-control" /></td>
                                                                          <td><input type="date" id="ExpiryDate" class="form-control" /></td>
                                                                          <td data-max-quantity="${item.PoQty}" style=" width:20%;">
                                                                    <div class="add-remove">
                                                                        <button class="decrementbtn btn" ><span>-</span></button>
                                                                        <input type="text" class="quantityInput" style="width:90px;" disabled value="${item.PoQty}">
                                                                        <button class="incrementbtn btn" >+</button>
                                                                    </div>
                                                                          </td>
                                                                          <td id="productid" style="display: none;">${item.productid}</td>
                                                                           <td id="podetail" style="display: none;">${item.podetail}</td>
                                                                          <td style="background-color: #e9ecef" id="uomtype">${item.UomName}</td>
                                                                          <td ><input type="text" id="UnitPrice" class="UnitPrice form-control"value="${item.UnitPrice}" /></td>
                                                                          <td style="background-color: #e9ecef" id="discount">${item.discountpercentage}</td>
                                                                          <td style="background-color: #e9ecef" id="discountvalue">${item.DiscountValue}</td>
                                                                          <td style="background-color: #e9ecef" id="subtotal">${item.SubtotalValue}</td>
                                                                          <td style="background-color: #e9ecef" id="taxper">${item.taxpercentage}</td>
                                                                          <td style="background-color: #e9ecef" id="TaxValue">${item.TaxValue}</td>
                                                                          <td style="background-color: #e9ecef" id="Grandtotal">${item.TotalValue}</td>
                                                                          <td style="background-color: #e9ecef">
                                                                              <button class="fill-color font-15 add" id="add">Add</button>
                                                                          </td>
                                                                      </tr>
                                                                                                `);
                        });
                    },
                    error: function (error) {
                        console.error('Error fetching PO details:', error);
                    }
                });
            });
        });
    </script>
    <script>

        $(document).ready(function () {
            $(document).on("click", ".add", function () {
                var currencyOption = $('#currencyToggle').is(':checked') ? 'USD' : 'ZAR';
                var row = $(this).closest('tr');
                var ProductNameId = row.find('#productid').text();
                var PoDetailId = row.find('#podetail').text();
                var UnitPrice = row.find('#UnitPrice').val();
                var BatchCode = row.find('#BatchCode').val();
                var ExpiryDate = row.find('#ExpiryDate').val();
                var Quantity = row.find('.quantityInput').val();
                var DiscountPerId = row.find('#discount').text();
                var PoHeaderId = $("#PoHeaderId").val();
                var totaltax = $("#Total_TaxValue").val();
                var totaldis = $("#Total_discountval").val();
                var totalsub = $("#Total_Subtotal").val();
                var totaldgrand = $("#Total_Grandtotal").val();
                var formdata = new FormData();
                formdata.append("ProductNameId", ProductNameId);
                formdata.append("PoDetailId", PoDetailId);
                formdata.append("DiscountPerId", DiscountPerId);
                formdata.append("Quantity", Quantity);
                formdata.append("UnitPrice", UnitPrice);
                formdata.append("BatchCode", BatchCode);
                formdata.append("ExpiryDate", ExpiryDate);
                formdata.append("currencyOption", currencyOption);

                formdata.append("Total_Subtotal", totalsub);
                formdata.append("PoHeaderId", PoHeaderId);
                formdata.append("Total_TaxValue", totaltax);
                formdata.append("Total_discountval", totaldis);
                formdata.append("Total_Grandtotal", totaldgrand);
                $.ajax({
                    url: '/GRN/GRNCartDetails',
                    type: 'POST',
                    data: formdata,
                    contentType: false,
                    processData: false,
                    dataType: 'html',
                    success: function (response) {
                        var data = JSON.parse(response);
                        row.find(".quantityInput").val("");
                        row.find("#UnitPrice").val("");
                        row.find('td[data-max-quantity]').attr('data-max-quantity', data.Grnqty);
                        row.find(".quantityInput").val(data.Grnqty);
                        row.find("#UnitPrice").val(data.Unitprices);
                        if (data.AlertMessage === "Added Successfully") {
                            Swal.fire({
                                title: 'Success!!',
                                text: 'Added Successfully!',
                                icon: 'success',
                                timer: 1500,
                                timerProgressBar: true,
                                showConfirmButton: false
                            });
                            $.ajax({
                                type: 'POST',
                                url: '/GRN/GRNcART',
                                data: { poheaderid: PoHeaderId },
                                contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                                processData: true,
                                success: function (response) {
                                    $("#grncart").empty();
                                    $.each(response.grnCarlist, function (i, item) {
                                        $("#grncart").append(`
                                                                    <tr>
                                                                        <td id="taxper">${i}</td>
                                                                        <td id="Grandtotal1">${item.productname}</td>
                                                                        <td id="taxper">${item.unitprice}</td>
                                                                        <td id="TaxValue1">${item.quantity}</td>
                                                                        <td id="Grandtotal1">${item.discountvalue}</td>
                                                                        <td id="taxper">${item.subtotal}</td>
                                                                        <td id="TaxValue1">${item.taxvalue}</td>
                                                                        <td id="Grandtotal1">${item.grandtotal}</td>
                                                                        <td class="veh-4" style="width:12%;">
                                                                            <span class="table_icons">
                                                                                <img src="/Content/assets/images/delete-icon.png" onclick="Delete('${item.grncartid}')" />
                                                                            </span>
                                                                        </td>
                                                                    </tr>
                                                                `);
                                    });
                                }
                            });

                        }
                    }

                });

            });
            $(document).on("keyup", ".UnitPrice", function () {
                var $row = $(this).closest('tr');
                var $quantityInput = $row.find('.quantityInput');
                var Quantity = parseInt($quantityInput.val(), 10);
                if (Quantity > 0) {
                    var currencyOption = $('#currencyToggle').is(':checked') ? 'USD' : 'ZAR';
                    var row = $(this).closest('tr');
                    var ProductNameId = row.find('#productid').text();
                    var UnitPrice = row.find('#UnitPrice').val();
                    var DiscountPerId = row.find('#discount').text();
                    var totaltax = $("#Total_TaxValue").val();
                    var totaldis = $("#Total_discountval").val();
                    var totalsub = $("#Total_Subtotal").val();
                    var totaldgrand = $("#Total_Grandtotal").val();
                    var formdata = new FormData();
                    formdata.append("ProductNameId", ProductNameId);
                    formdata.append("DiscountPerId", DiscountPerId);
                    formdata.append("Quantity", Quantity);
                    formdata.append("UnitPrice", UnitPrice);
                    formdata.append("currencyOption", currencyOption);

                    formdata.append("Total_Subtotal", totalsub);
                    formdata.append("Total_TaxValue", totaltax);
                    formdata.append("Total_discountval", totaldis);
                    formdata.append("Total_Grandtotal", totaldgrand);
                    $.ajax({
                        url: '/GRN/input_Calculationdetails',
                        type: 'POST',
                        data: formdata,
                        contentType: false,
                        processData: false,
                        dataType: 'html',
                        success: function (response) {
                            row.find("#subtotal").text("");
                            row.find("#TaxValue").text("");
                            row.find("#Grandtotal").text("");
                            row.find("#discountamount").text("");
                            $("#Total_Subtotal").val("");
                            $("#Total_discountval").val("");
                            $("#Total_TaxValue").val("");
                            $("#Total_Grandtotal").val("");
                            var data = JSON.parse(response);
                            row.find("#subtotal").text(data.SubTotal);
                            row.find("#TaxValue").text(data.TaxValue);
                            row.find("#Grandtotal").text(data.GrandTotal);
                            row.find("#discountamount").text(data.DiscountValue);

                            $("#Total_discountval").val(data.Total_discountval);
                            $("#Total_Subtotal").val(data.Total_Subtotal);
                            $("#Total_TaxValue").val(data.Total_TaxValue);
                            $("#Total_Grandtotal").val(data.Total_Grandtotal);
                        }

                    });
                }

            });
            $(document).on("click", ".decrementbtn", function () {
                var $quantityInput = $(this).siblings('.quantityInput');
                var quantityValue = parseInt($quantityInput.val(), 10);
                if (quantityValue > 0) {
                    $quantityInput.val(quantityValue - 1);
                    var Quantity = (quantityValue - 1);
                    var currencyOption = $('#currencyToggle').is(':checked') ? 'USD' : 'ZAR';
                    var row = $(this).closest('tr');
                    var ProductNameId = row.find('#productid').text();
                    var UnitPrice = row.find('#UnitPrice').val();
                    var DiscountPerId = row.find('#discount').text();
                    var totaltax = $("#Total_TaxValue").val();
                    var totaldis = $("#Total_discountval").val();
                    var totalsub = $("#Total_Subtotal").val();
                    var totaldgrand = $("#Total_Grandtotal").val();
                    var formdata = new FormData();
                    formdata.append("ProductNameId", ProductNameId);
                    formdata.append("DiscountPerId", DiscountPerId);
                    formdata.append("Quantity", Quantity);
                    formdata.append("UnitPrice", UnitPrice);
                    formdata.append("currencyOption", currencyOption);

                    formdata.append("Total_Subtotal", totalsub);
                    formdata.append("Total_TaxValue", totaltax);
                    formdata.append("Total_discountval", totaldis);
                    formdata.append("Total_Grandtotal", totaldgrand);
                    $.ajax({
                        url: '/GRN/Dec_Calculationdetails',
                        type: 'POST',
                        data: formdata,
                        contentType: false,
                        processData: false,
                        dataType: 'html',
                        success: function (response) {
                            row.find("#subtotal").text("");
                            row.find("#TaxValue").text("");
                            row.find("#Grandtotal").text("");
                            row.find("#discountamount").text("");
                            $("#Total_Subtotal").val("");
                            $("#Total_discountval").val("");
                            $("#Total_TaxValue").val("");
                            $("#Total_Grandtotal").val("");
                            var data = JSON.parse(response);
                            row.find("#subtotal").text(data.SubTotal);
                            row.find("#TaxValue").text(data.TaxValue);
                            row.find("#Grandtotal").text(data.GrandTotal);
                            row.find("#discountamount").text(data.DiscountValue);

                            $("#Total_discountval").val(data.Total_discountval);
                            $("#Total_Subtotal").val(data.Total_Subtotal);
                            $("#Total_TaxValue").val(data.Total_TaxValue);
                            $("#Total_Grandtotal").val(data.Total_Grandtotal);
                        }

                    });
                }

            });
            $(document).on("click", ".incrementbtn", function () {
                var $quantityInput = $(this).siblings('.quantityInput');
                var quantityValue = parseInt($quantityInput.val(), 10);
                var maxQuantity = $(this).closest('td').data('max-quantity');
                if (quantityValue < maxQuantity) {
                    $quantityInput.val(quantityValue + 1);
                    var Quantity = (quantityValue + 1);
                    var currencyOption = $('#currencyToggle').is(':checked') ? 'USD' : 'ZAR';
                    var row = $(this).closest('tr');
                    var ProductNameId = row.find('#productid').text();
                    var UnitPrice = row.find('#UnitPrice').val();
                    var DiscountPerId = row.find('#discount').text();
                    var totaltax = $("#Total_TaxValue").val();
                    var totaldis = $("#Total_discountval").val();
                    var totalsub = $("#Total_Subtotal").val();
                    var totaldgrand = $("#Total_Grandtotal").val();
                    var formdata = new FormData();
                    formdata.append("ProductNameId", ProductNameId);
                    formdata.append("DiscountPerId", DiscountPerId);
                    formdata.append("Quantity", Quantity);
                    formdata.append("currencyOption", currencyOption);
                    formdata.append("UnitPrice", UnitPrice);
                    formdata.append("Total_Subtotal", totalsub);
                    formdata.append("Total_TaxValue", totaltax);
                    formdata.append("Total_discountval", totaldis);
                    formdata.append("Total_Grandtotal", totaldgrand);
                    $.ajax({
                        url: '/GRN/Inc_Calculationdetails',
                        type: 'POST',
                        data: formdata,
                        contentType: false,
                        processData: false,
                        dataType: 'html',
                        success: function (response) {
                            row.find("#subtotal").text("");
                            row.find("#TaxValue").text("");
                            row.find("#Grandtotal").text("");
                            row.find("#discountamount").text("");
                            $("#Total_Subtotal").val("");
                            $("#Total_discountval").val("");
                            $("#Total_TaxValue").val("");
                            $("#Total_Grandtotal").val("");
                            var data = JSON.parse(response);
                            row.find("#subtotal").text(data.SubTotal);
                            row.find("#TaxValue").text(data.TaxValue);
                            row.find("#Grandtotal").text(data.GrandTotal);
                            row.find("#discountamount").text(data.DiscountValue);

                            $("#Total_discountval").val(data.Total_discountval);
                            $("#Total_Subtotal").val(data.Total_Subtotal);
                            $("#Total_TaxValue").val(data.Total_TaxValue);
                            $("#Total_Grandtotal").val(data.Total_Grandtotal);
                        }

                    });
                }

            });
            $("#SupplierId").change(function () {
                var SupplierId = $("#SupplierId").val();
                $.ajax({
                    type: 'GET',
                    url: '/GRN/Getpono',
                    data: { id: SupplierId },
                    contentType: 'application/json',
                    success: function (values) {
                        $("#PoHeaderId").empty();

                        $.each(values, function (i, item) {
                            $("#PoHeaderId").append(
                                $('<option></option>').val(item.PoheaderId).html(item.PoheaderId));
                        });
                    }
                });
            });

        });
    </script>

    <script>
        function cancel() {
            Swal.fire({
                title: "Are you sure?",
                text: "You want to cancel the GRN!",
                icon: "warning",
                showCancelButton: true,
                confirmButtonColor: "#3085d6",
                cancelButtonColor: "#d33",
                confirmButtonText: "Yes,cancel it!"
            }).then((result) => {
                if (result.isConfirmed) {
                    Swal.fire({
                        title: "Deleted!",
                        text: "Your GRN has been deleted successfully",
                        icon: "success"
                    }).then(() => {
                        window.location.href = "/GRN/GRNMaster";
                    });
                }
            });
        }
       
    </script>
</body>

</html>
