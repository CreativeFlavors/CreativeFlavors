@model MMS.Web.Models.JobworkModel.JobSheet_pairCodeModel
@using MMS.Repository.Managers.JobWork
@{
    List<MMS.Core.Entities.JobWork.JobSheet_pairCodeMaster> JobSheet_pairCodeList = new List<MMS.Core.Entities.JobWork.JobSheet_pairCodeMaster>().ToList();
    JobSheet_PairManager JobSheet_PairManager = new JobSheet_PairManager();

}

<div class="veh-tablecont">
    <div class="header_list_tab  col-lg-12 col-md-12 col-sm-12  col-xs-12">
        <div class="col-lg-12 col-md-12 col-sm-12 col-12 no-padding">
            <div class="list_tab">
                <div class="title">
                    List
                </div>
                <button class="rt_add" onclick="AddSheet_toLeather()">Add</button>
            </div>

        </div>
        <div class="col-lg-6 col-md-12 col-sm-12  col-xs-12">
            <div class="button_tab  col-lg-12 col-md-12 col-sm-12 col-12 no-padding">

            </div>
        </div>
    </div>

    <div class="process_master col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="col-xs-12 block-section">
            <div id="no-more-tables" class="col-xs-12 no-padding">
                <table id="example" class="col-md-12 table-striped table-condensed cf table-change home_dahboard home_process">
                    <thead>
                        <tr>
                            <th>SI.No</th>
                            <th>Jobsheet pair Code</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody class="common-table-head">
                        @if (Model.JobSheet_pairCodeMasterlist != null && Model.JobSheet_pairCodeMasterlist.Count > 0)
                        {
                            int count = 1;
                            if (JobSheet_pairCodeList == null || JobSheet_pairCodeList.Count == 0)
                            {
                                foreach (var JobSheet_pairCodeMasterlist in Model.JobSheet_pairCodeMasterlist)
                                {
                                  
                                    <tr>
                                        <td data-title="Si.No">@count</td>
                                        <td data-title="OperationType Code">@JobSheet_pairCodeMasterlist.jobsheet_pair_Code</td>

                                        <td data-title="Action">
                                            <span class="action-icons">

                                                <a><img onclick="Edit('@JobSheet_pairCodeMasterlist.jobsheet_pair_code_id')" src="~/Scripts/Jobwork/images/edit.png"></a>
                                                <img onclick="Delete('@JobSheet_pairCodeMasterlist.jobsheet_pair_code_id')" src="~/Scripts/Jobwork/images/delete.png">
                                            </span>
                                        </td>
                                    </tr>
                                    count++;
                                }
                            }
                        }
                        else
                        {
                            int count = 1;
                            foreach (var Stage in JobSheet_pairCodeList)
                            {

                                <tr>
                                    <td class="veh-1">@count</td>
                                    <td class="veh-1" onclick="">@Stage.jobsheet_pair_Code/td>
                                    <td data-title="Action">
                                        <span class="action-icons">
                                            <a href="Home_Process_Master_edit.html"><img src="~/Scripts/Jobwork/images/edit.png"></a>
                                            <img src="~/Scripts/Jobwork/images/delete.png">
                                        </span>
                                    </td>
                                </tr>
                                count++;
                            }

                            JobSheet_pairCodeList = null;
                        }
                        </>
                </table>
            </div>
        </div>

    </div>
</div>
<div id="EditSheet_topairPartial">
    @*@Html.Partial("~/Views/Jobwork/Job_Master/ProcessMaster/Partial/ProcessEdit.cshtml", Model)*@
</div>



