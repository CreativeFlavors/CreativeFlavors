@model List<MMS.Core.Entities.JobWork.JobSheet_PairMaster>

@{
    int count = Model.Count;

}


@if (Model.Count >0)
{
    foreach (var item in Model)
    {
        <tr>
            <td>@item.jobsheet_pair_id</td>
            <td>@item.Buyer</td>
            <td>@item.Material</td>
            <td>@item.Qty </td>
            <td>@item.Season</td>
            <td>@item.Stores</td>
            <td>
                <span class="action-icons">
                    <img src="~/Scripts/Jobwork/images/edit.png" />
                    <img src="~/Scripts/Jobwork/images/delete.png" />
                </span>
            </td>

        </tr>
    }
}
