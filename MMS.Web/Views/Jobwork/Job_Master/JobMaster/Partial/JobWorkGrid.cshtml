@model MMS.Web.Models.JobworkModel.JobworkModel
@using MMS.Repository.Managers.JobWork;
@using MMS.Core.Entities;
@{
    List<MMS.Core.Entities.JobWork.JobworkMaster> Jobworklist = new List<MMS.Core.Entities.JobWork.JobworkMaster>().ToList();
    JobworkManager JobworkManager = new JobworkManager();

}
<div class="veh-tablecont">
    <div class="header_list_tab  col-lg-12 col-md-12 col-sm-12  col-xs-12">
        <div class="col-lg-12 col-md-12 col-sm-12 col-12 no-padding">
            <div class="list_tab">
                <div class="title">
                    List
                </div>
                <button class="rt_add" onclick="AddJob()">Add</button>
            </div>

        </div>
        <div class="col-lg-6 col-md-12 col-sm-12  col-xs-12">
            <div class="button_tab  col-lg-12 col-md-12 col-sm-12 col-12 no-padding">

            </div>
        </div>
    </div>

    <div class="process_master col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="col-xs-12 block-section">
            <div id="no-more-tables" class="col-xs-12 no-padding">
                <table id="example" class="col-md-12 table-striped table-condensed cf table-change home_dahboard home_process">
                    <thead>
                        <tr>
                            <th>Si.No</th>
                            <th>JW Code</th>
                            <th>JW Name</th>
                            <th>Currency</th>
                            <th>Address</th>
                            <th>Country</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody class="common-table-head">
                        @if (Model.JobworkMasterList != null && Model.JobworkMasterList.Count > 0)
                        {
                            int count = 1;
                            if (Jobworklist == null || Jobworklist.Count == 0)
                            {
                                foreach (var Job in Model.JobworkMasterList)
                                {
                        <tr>
                            <td data-title="Si.No">@count</td>
                            <td data-title="JW Code">@Job.JW_Code</td>
                            <td data-title="JW Name">@Job.JW_Name</td>
                            <td data-title="Currency">@Job.Currency</td>
                            <td data-title="Address">@Job.Address</td>
                            <td data-title="Country">@Job.Country</td>


                            <td data-title="Action">
                                <span class="action-icons">

                                    <a><img onclick="Edit('@Job.JW_Id')" src="~/Scripts/Jobwork/images/edit.png"></a>
                                    <img onclick="Delete('@Job.JW_Id')" src="~/Scripts/Jobwork/images/delete.png">
                                </span>
                            </td>
                        </tr>
                                    count++;
                                }
                            }
                        }
                        else
                        {
                            int count = 1;
                            foreach (var Job in Jobworklist)
                            {

                        <tr>
                            <td class="veh-1">@count</td>
                            <td class="veh-2" onclick="">@Job.JW_Code/td>
                            <td class="veh-3">@Job.JW_Name</td>
                            <td class="veh-4" onclick="">@Job.Currency</td>
                            <td class="veh-5">@Job.Address</td>
                            <td class="veh-5">@Job.Country</td>
                            <td data-title="Action">
                                <span class="action-icons">
                                    <a href="Home_Process_Master_edit.html"><img src="~/Scripts/Jobwork/images/edit.png"></a>
                                    <img src="~/Scripts/Jobwork/images/delete.png">
                                </span>
                            </td>
                        </tr>
                                count++;
                            }

                            Jobworklist = null;
                        }
                        </>
                </table>
            </div>
        </div>

    </div>
</div>
<div id="EditJobPartial">
    @*@Html.Partial("Partial/OperationMasterDetails", Model)*@
</div>