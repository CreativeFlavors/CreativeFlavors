@model MMS.Web.Models.JobworkModel.ProductionJobworkModel
@using MMS.Repository.Managers.JobWork;
@using MMS.Core.Entities;
@{
    List<MMS.Data.StoredProcedureModel.JobProductionGrid> JobProductionGrid = new List<MMS.Data.StoredProcedureModel.JobProductionGrid>();
    List<MMS.Core.Entities.JobWork.ProductionJobworkMaster> ProductionJobworkMaster = new List<MMS.Core.Entities.JobWork.ProductionJobworkMaster>().ToList();

}
<div class="veh-tablecont">
    <div class="header_list_tab  col-lg-12 col-md-12 col-sm-12  col-xs-12">
        <div class="col-lg-12 col-md-12 col-sm-12 col-12 no-padding">
            <div class="list_tab">
                <div class="title">
                    List
                </div>
                <button class="rt_add" onclick="Add_Production()">Add</button>
            </div>

        </div>
        <div class="col-lg-6 col-md-12 col-sm-12  col-xs-12">
            <div class="button_tab  col-lg-12 col-md-12 col-sm-12 col-12 no-padding">

            </div>
        </div>
    </div>

    <div class="process_master col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="col-xs-12 block-section">
            <div id="no-more-tables" class="col-xs-12 no-padding">
                <table id="example" class="col-md-12 table-striped table-condensed cf table-change home_dahboard home_process">
                    <thead>
                        <tr>
                            <th>Si.No</th>
                            <th>Production Code</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody class="common-table-head">
                        @if (Model.ProductionJobwork_Code_list != null )
                        {
                            int count = 1;
                            if (JobProductionGrid == null || JobProductionGrid.Count == 0)
                            {
                                foreach (var Job in Model.ProductionJobwork_Code_list)
                                {
                        <tr>
                            <td data-title="Si.No">@count</td>
                            <td data-title="JW Code">@Job.ProductionJobwork_Code</td>


                            <td data-title="Action">
                                <span class="action-icons">

                                    <a><img onclick="Edit('@Job.ProductionJobwork_code_id')" src="~/Scripts/Jobwork/images/edit.png"></a>
                                    <img onclick="Delete('@Job.ProductionJobwork_code_id')" src="~/Scripts/Jobwork/images/delete.png">
                                </span>
                            </td>
                        </tr>
                                    count++;
                                }
                            }
                        }
                        else
                        {
                            int count = 1;
                            foreach (var Job in ProductionJobworkMaster)
                            {

                        <tr>
                            <td class="veh-1">@count</td>
                            <td class="veh-2" onclick="">@Job.Prod_code_id/td>
                            <td class="veh-3">@Job.Jw_Name</td>
                            <td class="veh-4" onclick="">@Job.Process_Name</td>
                            <td class="veh-5">@Job.Stage_From</td>
                            <td class="veh-5">@Job.Stage_To</td>
                            <td class="veh-5">@Job.Lot_no</td>
                            <td data-title="Action">
                                <span class="action-icons">
                                    <a href="Home_Process_Master_edit.html"><img src="~/Scripts/Jobwork/images/edit.png"></a>
                                    <img src="~/Scripts/Jobwork/images/delete.png">
                                </span>
                            </td>
                        </tr>
                                count++;
                            }

                            JobProductionGrid = null;
                        }
                        </>
                </table>
            </div>
        </div>

    </div>
</div>
<div id="EditProductionPartial">
    @*@Html.Partial("Partial/OperationMasterDetails", Model)*@
</div>