@model MMS.Web.Models.JobworkModel.JobLeather_leatherModel
@using MMS.Repository.Managers.JobWork;
@using MMS.Core.Entities;
@{
    /**/

    List<MMS.Data.StoredProcedureModel.Job_LetherCode> JobLeather_leatherMaster = new List<MMS.Data.StoredProcedureModel.Job_LetherCode>().ToList();
    JobLeather_leatherManager JobLeather_leatherManager = new JobLeather_leatherManager();

}

<div class="veh-tablecont">
    <div class="header_list_tab  col-lg-12 col-md-12 col-sm-12  col-xs-12">
        <div class="col-lg-12 col-md-12 col-sm-12 col-12 no-padding">
            <div class="list_tab">
                <div class="title">
                    List
                </div>
                <button class="rt_add" onclick="AddJobLeather()">Add</button>
            </div>

        </div>
        <div class="col-lg-6 col-md-12 col-sm-12  col-xs-12">
            <div class="button_tab  col-lg-12 col-md-12 col-sm-12 col-12 no-padding">

            </div>
        </div>
    </div>

    <div class="process_master col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="col-xs-12 block-section">
            <div id="no-more-tables" class="col-xs-12 no-padding">
                <table id="example" class="col-md-12 table-striped table-condensed cf table-change home_dahboard home_process">
                    <thead>
                        <tr>
                            <th>Si.No</th>
                            <th>JW Leather No</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody class="common-table-head">
                        @if (Model.Job_LetherCode != null && Model.Job_LetherCode.Count > 0)
                        {
                            int count = 1;
                            if (JobLeather_leatherMaster == null || JobLeather_leatherMaster.Count == 0)
                            {
                                foreach (var Job in Model.Job_LetherCode)
                                {
                                    <tr>
                                        <td data-title="Si.No">@count</td>
                                        <td data-title="JW Code">@Job.Job_Lether_to_lether_Code</td>


                                        <td data-title="Action">
                                            <span class="action-icons">

                                                <a><img onclick="Edit('@Job.Job_Lether_to_lether_Code')" src="~/Scripts/Jobwork/images/edit.png"></a>
                                                <img onclick="Delete('@Job.Job_Lether_to_lether_Code')" src="~/Scripts/Jobwork/images/delete.png">
                                            </span>
                                        </td>
                                    </tr>
                                    count++;
                                }
                            }
                        }
                        else
                        {
                            int count = 1;
                            foreach (var Job in JobLeather_leatherMaster)
                            {

                                <tr>
                                    <td class="veh-1">@count</td>
                                    <td class="veh-2" onclick="">@Job.Job_Lether_to_lether_Code/td>
                                    <td data-title="Action">
                                        <span class="action-icons">
                                            <a href="Home_Process_Master_edit.html"><img src="~/Scripts/Jobwork/images/edit.png"></a>
                                            <img src="~/Scripts/Jobwork/images/delete.png">
                                        </span>
                                    </td>
                                </tr>
                                count++;
                            }

                            JobLeather_leatherMaster = null;
                        }
                        </>
                </table>
            </div>
        </div>

    </div>
</div>
<div id="EditJobLeather_leatherPartial">
    @*@Html.Partial("Partial/OperationMasterDetails", Model)*@
</div>