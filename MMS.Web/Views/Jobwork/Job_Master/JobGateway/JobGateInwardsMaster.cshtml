                });
                $('#table_grid_ROW tr').find('.InternalValue_general').each(function () {
                    //  $(this).val()

                 
                    qty += Number($(this).text());

                });
                
                if (parseFloat(qty) < parseFloat(sum)) {
                    debugger
                    swal("Quantity is not equal!", "", "info");
                    bool = false;
                }
                sum = 0;
            });

        }
        return bool;
    }

    
