    $("#StoreMasterId").combobox({
        select: function (event, ui) {



            if ($('#StoreMasterId option:selected').text().trim() == "Maintenance Store") {
                $('#MachineryMasterId').removeAttr("disabled");
            }
            else {

                $('#MachineryMasterId').attr("disabled", "disabled");
            }

            var stateId = $('#StoreMasterId').val();
            $.ajax({
                url: '/MaterialMaster/FillMaterialCateogoryid',
                type: "GET",
                dataType: "JSON",
                data: { StoreMasterId: stateId },
                success: function (cities) {
                    $("#MaterialCategoryMasterId").html("");
                    $("#materialCategoryId_ .custom-combobox input").val('');

                    // clear before appending new list
                    $.each(cities, function (i, city) {
                        $("#MaterialCategoryMasterId").append(
                            $('<option></option>').val(city.MaterialCategoryMasterId).html(city.CategoryName));
                    });
                }
            });
        }
    });
    $("#MaterialCategoryMasterId").combobox({
        select: function (event, ui) {


