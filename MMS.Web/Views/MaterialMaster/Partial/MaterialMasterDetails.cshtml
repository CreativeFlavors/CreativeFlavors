                        $("#MaterialGroupMasterId").append(
                            $('<option></option>').val(city.MaterialGroupMasterId).html(city.GroupName));

                    });
                }
            });
        }
    });


    $("#MaterialGroupMasterId").combobox({
        select: function (event, ui) {

            $('#GradeMasterId').attr("disabled", false);
            $('#SubstanceMasterId').attr("disabled", false);
            $('#LeatherSizeMasterId').attr("disabled", false);
            $('#OriginMasterId').attr("disabled", false);
            $('#SizeRangeMasterId').attr("disabled", false);
            $('#ColorMasterId').attr("disabled", false);
            $('#Barcode').attr("disabled", false);
            $.ajax({
                type: "POST",
                url: ' @Url.Action("MaterialGroup", "MaterialMaster")',
                data: { MaterialGroupMasterId: $('#MaterialGroupMasterId').val() },
                success: function (data) {

                    if (data.IsColor == true) {
                        $('#ColorMasterId').attr("disabled", false);
                    }
                   else if (data.IsColor == false) {
                        $('#ColorMasterId').attr("disabled", true);
                    }
                    if (data.IsConsumption == true) {
                        // $('#ColorMasterId').attr("disabled", true);
                    }
                    else {
                    }
                    if (data.IsSubstance == true) {
                        $('#GradeMasterId').attr("disabled", false);
                        $('#SubstanceMasterId').attr("disabled", false);
                        $('#LeatherSizeMasterId').attr("disabled", false);
                        $('#OriginMasterId').attr("disabled", false);
                    }
                    else if ((data.IsSubstance == false)) {
                        $('#GradeMasterId').attr("disabled", true);
                        $('#SubstanceMasterId').attr("disabled", true);
                        $('#LeatherSizeMasterId').attr("disabled", true);
                        $('#OriginMasterId').attr("disabled", true);
                    }
                    if (data.IsSize == true) {
                        $('#SizeRangeMasterId').attr("disabled", false);
                    }
                    else if (data.IsSize == false) {
                        $('#SizeRangeMasterId').attr("disabled", true);
                    }
                    if (data.IsInspection == true) {
                        //$('#ColorMasterId').attr("disabled", true);
                    }
                    if (data.IsReservation == true) {
                        // $('#ColorMasterId').attr("disabled", true);
                    }
                    if (data.IsDisplay == true) {
                        //$('#ColorMasterId').attr("disabled", true);
                    }
                    if (data.IsBatchcode == true) {
                        $('#Barcode').attr("disabled", true);
                    }
                    else if (data.IsBatchcode == false) {
                        $('#Barcode').attr("disabled", false);
                    }
                    if (data.IsMultipleUnits == true) {
                        //$('#ColorMasterId').attr("disabled", true);
                    }
                }
            });

            var MaterialGroupMasterId_ = $('#MaterialGroupMasterId').val();
            $.ajax({
                url: '/MaterialMaster/FillMaterialMasterName',
                type: "GET",
                dataType: "JSON",
                data: { MaterialGroupMasterId: MaterialGroupMasterId_ },
                success: function (cities) {
                    console.log(cities);
                    $("#MaterialName").html(""); // clear before appending new list
                    $.each(cities, function (i, city) {
                        $("#MaterialName").append(
                            $('<option></option>').val(city.MaterialMasterID).html(city.MaterialDescription));

                    });


                }
            });
            $.ajax({
                type: 'POST',
                url: ' @Url.Action("GetMaterialCode", "MaterialMaster")',
                data: { MaterialGroupMasterId: $('#MaterialGroupMasterId').val() },
                success: function (data) {
                    $('#MaterialCode').val(data)
                },
                error: function (ex) {
                    alert('Error');
                }
            });
        }

    });

    //});

    // A $( document ).ready() block.

    $('#Cancel').hide();
    $('#First').show();
    $('#Second').hide();
    $('#save').hide();
    $('#MaterialMasterId').hide();
    $('#btn_Next').show();
    $('#btn_Back').hide();

    function Next() {
        $('#Cancel').show();
        $('#First').hide();
        $('#Second').show();
        $('#save').show();
        $('#MaterialMasterId').show();
        $('#btn_Next').hide();
        $('#btn_Back').show();
    }
    function Back() {
        $('#Cancel').hide();
        $('#First').show();
        $('#Second').hide();
        $('#save').hide();
        $('#MaterialMasterId').hide();
        $('#btn_Next').show();
        $('#btn_Back').hide();
    }
    $(".allownumericwithdecimal").on("keypress keyup blur", function (event) {

        $(this).val($(this).val().replace(/[^0-9\.]/g, ''));
        if ((event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 48 || event.which > 57)) {
            event.preventDefault();
        }
    });
</script>

