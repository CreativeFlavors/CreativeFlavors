<script>
    $('.drilldown').drilldown();  
    var win_height = $(window).height() - 79;
    $(".left-side-bar").css({ "height": win_height });
    var _cont_height = $(window).height() - 79;
    var _cont_width = $(window).width() - 250;
    $(".content-wrapper").css({ "height": _cont_height, "width": _cont_width });
    $(window).load(function () {
        var win_height = $(window).height() - 79;
        $(".left-side-bar").css({ "height": win_height });
        var _cont_height = $(window).height() - 79;
        var _cont_width = $(window).width() - 285;
        $(".content-wrapper").css({ "height": _cont_height, "width": _cont_width });
    });
    $('colgroup').hide();
    $("#pager").css({ "position": "relative", "top": "auto" });
    $(window).resize(function () {
        var win_height = $(window).height() - 79;
        $(".left-side-bar").css({ "height": win_height });
        var _cont_height = $(window).height() - 79;
        var _cont_width = $(window).width() - 250;
        $(".content-wrapper").css({ "height": _cont_height, "width": _cont_width });
    });    
    var specialKeys = new Array();
    specialKeys.push(8); //Backspace
    $(function () {       
        $(".numeric").bind("keypress", function (e) {
            var keyCode = e.which ? e.which : e.keyCode
            var ret = ((keyCode >= 48 && keyCode <= 57) || specialKeys.indexOf(keyCode) != -1);
            $(".error").css("display", ret ? "none" : "inline");
            return ret;
        });
        $(".numeric").bind("paste", function (e) {
            return false;
        });
        $(".numeric").bind("drop", function (e) {
            return false;
        });

        var winPath = window.location.pathname;
        $(".left-side-bar ul li a").each(function () {
            if ($(this).attr("href") == winPath) {
                //console.log($(this).attr("href"));
                //console.log(winPath);
                $(this).parents("li.has-children").find("input[type='checkbox']").prop("checked", true);
                $(this).addClass("current-menu");
            }
        });
        $(".cd-accordion-menu > li").on("click", function () {
            $(this).siblings().removeClass("active");
            $(this).addClass("active");
        });
    });



</script>


