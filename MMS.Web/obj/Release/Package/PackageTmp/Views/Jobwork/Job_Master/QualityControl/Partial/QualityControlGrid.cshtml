@model MMS.Web.Models.JobworkModel.ProductionQcModel
@using MMS.Repository.Managers.JobWork;
@using MMS.Core.Entities;
@{
    
    List<MMS.Core.Entities.JobWork.Production_QcCodeMaster> Production_QcCodeMaster = new List<MMS.Core.Entities.JobWork.Production_QcCodeMaster>().ToList();

}
<script>
    $(".chosen-select").chosen();

</script>
<div class="veh-tablecont">
    <div class="header_list_tab  col-lg-12 col-md-12 col-sm-12  col-xs-12">
        <div class="col-lg-12 col-md-12 col-sm-12 col-12 no-padding">
            <div class="list_tab">
                <div class="title">
                    List
                </div>
                <button class="rt_add" onclick="Add_Qc()">Add</button>
            </div>

        </div>
        <div class="col-lg-6 col-md-12 col-sm-12  col-xs-12">
            <div class="button_tab  col-lg-12 col-md-12 col-sm-12 col-12 no-padding">

            </div>
        </div>
    </div>

    <div class="process_master col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="col-xs-12 block-section">
            <div id="no-more-tables" class="col-xs-12 no-padding">
                <table id="example" class="col-md-12 table-striped table-condensed cf table-change home_dahboard home_process">
                    <thead>
                        <tr>
                            <th>Si.No</th>
                            <th>Production Code</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody class="common-table-head">
                        @if (Model.Production_QcCodeMaster != null)
                        {
                            int count = 1;
                            if (Production_QcCodeMaster == null || Production_QcCodeMaster.Count == 0)
                            {
                                foreach (var Job in Model.Production_QcCodeMaster)
                                {
                                    <tr>
                                        <td data-title="Si.No">@count</td>
                                        <td data-title="JW Code">@Job.Production_Code</td>


                                        <td data-title="Action">
                                            <span class="action-icons">

                                                <a><img onclick="Edit('@Job.ProductionQc_ID')" src="~/Scripts/Jobwork/images/edit.png"></a>
                                                <img onclick="Delete('@Job.ProductionQc_ID')" src="~/Scripts/Jobwork/images/delete.png">
                                            </span>
                                        </td>
                                    </tr>
                                    count++;
                                }
                            }
                        }
                        else
                        {
                            int count = 1;
                            foreach (var Job in Production_QcCodeMaster)
                            {

                                <tr>
                                    <td class="veh-1">@count</td>
                                    <td class="veh-2" onclick="">@Job.Production_Code/td>
                                    <td class="veh-3">@Job.ProductionQc_ID</td>
                                    <td data-title="Action">
                                        <span class="action-icons">
                                            <a href="Home_Process_Master_edit.html"><img src="~/Scripts/Jobwork/images/edit.png"></a>
                                            <img src="~/Scripts/Jobwork/images/delete.png">
                                        </span>
                                    </td>
                                </tr>
                                count++;
                            }

                            Production_QcCodeMaster = null;
                        }
                        </>
                </table>
            </div>
        </div>

    </div>
</div>
<div id="EditProductionPartial">
    @*@Html.Partial("Partial/OperationMasterDetails", Model)*@
</div>