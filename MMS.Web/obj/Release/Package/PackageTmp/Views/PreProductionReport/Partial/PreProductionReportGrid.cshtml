
@model MMS.Web.Models.Temp_Production.Temp_ProductionModel
@using MMS.Repository.Managers
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/css1/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css1/custom.css" rel="stylesheet" />
    <link href="~/Content/css1/responsive.css" rel="stylesheet" />
    <link href="~/Content/css1/variables.css" rel="stylesheet" />
    <style>

        .page-item.active .page-link {
            z-index: 3;
            color: #fff;
            background-color: #4472C4;
            border-color: #0d6efd;
        }

        .manage_customers .table tr th, .manage_customers .table tr td, .common_Manage_subMerchant .table tr th, .common_Manage_subMerchant .table tr td {
            padding: 6px;
        }

        #reports_search4_container button, #transaction_reports_search4_container button, #static_report_search4_container button {
            padding: 12px 29px;
            color: #FFFFFF;
        }
    </style>
</head>
<body>
    <div class="main-page">
        <div id="transaction_report" class="reports common_Manage_subMerchant">
            <div class="d-flex justify-content-between align-items-center manage_customers_head">
                <div class="manage_customers_main_head">
                    <h6 class="font-20">Pre-Production</h6>
                </div>
                <div class="radioSection">
                    <label for="add">
                        <input type="radio" name="yes" id="add" style="cursor: pointer;" checked>
                        Details
                    </label>
                    <label for="add1">
                        <input type="radio" name="yes" id="add1" style="cursor: pointer;">
                        Summery
                    </label>
                </div>
                <div>
                </div>
            </div>
            <div>
                <div class="" id="Detailgrid">
                    @{Html.RenderPartial("~/Views/PreProductionReport/Partial/PreProductionReportDetailsGrid.cshtml",Model);}
                </div>
                <div class="" id="summerygrid">
                    @{Html.RenderPartial("~/Views/PreProductionReport/Partial/PreProductionReportHeaderGrid.cshtml", Model);}
                </div>
            </div>

        </div>
    </div>


    <script src="~/Content/js/custom.js"></script>


    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

    <script src="~/Content/js/bootstrap.bundle.min.js"></script>
    <script>
        $(document).ready(function () {
            $("#summerygrid").hide();

        });
    </script>

    <script>
        $("#summerygrid").hide();
        $("#add1").click(function () {
            $("#summerygrid").show();
            $("#Detailgrid").hide();
        });
        $("#add").click(function () {
            $("#summerygrid").hide();
            $("#Detailgrid").show();
        });

    </script>
    <script>
        $(window).on("load", function () {
            $(".report-inner-section").addClass("transection");
            $("#order").attr('checked', true);
        });
    </script>


</body>
</html>


