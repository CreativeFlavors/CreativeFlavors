    function Edit(IssueId) {
        $.getJSON('/IssueSlip_Single/GetIssueDetails/', { IssueSlipId: IssueId }, function (data) {
            $("#btnSave").val("Update");
            var Date1 = $.datepicker.formatDate('mm/dd/yy', new Date(Date(data.Date)));
            $("#IssuesSlipId").val(data.IssuesSlipId);
             $("#ddlIssueType").val(data.IssuesTypeId);
             $("#txtSIssueSlipNo").val(data.IssuesSlipNo);
             $("#txtDate").val(Date1);
             $("#ddlConveyor").val(data.ConveyorId);
             $("#InternalOrderId").val(data.InternalOrderId);
             $("#MachineNameId").val(data.MachineNameId);
             $("#txtStyleNo").val(data.StyleNo);
             $("#txtNoOfsetsPairs").val(data.NoOfSets_Pairs);
             $("#StoresId").val(data.StoresId);
             $("#RateId").val(data.RateId);
             $("#CategoryId").val(data.CategoryId);
             $("#SubstanceId").val(data.SubstanceId);
             $("#GroupId").val(data.GroupId);
             $("#txtReqQty").val(data.RequiredQty);
             $("#MaterialId").val(data.MaterialId);
             $("#ColourId").val(data.ColourId);
             $("#txtAlreadyIssued").val(data.AlreadyIssued);
             $("#txtLotNo").val(data.LotNo);
             $("#txtCurrentIssues").val(data.CurrentIssues);
             $("#ddlCurrentSharesVal").val(data.CurrentIssuesVal);
             $("#txtBalanceILOTSQFT").val(data.BalanceInThisLot_Sqft);
             $("#txtBalanceILOTSPCS").val(data.BalanceInThisLot_Pcs);
             $("#txtCurrentStock").val(data.CurrentStock);
             $("#txtCurrentStockUOM").val(data.CurrentStock_Uom);
             $("#txtFreeStock").val(data.FreeStock);
             $("#txtFreeStockUOM").val(data.FreeStock_Uom);
             $("#txtReservedStock").val(data.ReservedStock);
             $("#txtReservedStockUOM").val(data.ReservedStock_Uom);
             $("#txtClosingStock").val(data.ClosingStockInAllStores);
             $("#txtClosingStockUOM").val(data.ClosingStockInAllStores_Uom);
             $("#txtInTransitStock").val(data.InTransit);
             $("#txtInTransitStockUOM").val(data.InTransit_Uom);
             $("#GridDetails").hide();
             $("#div_show1").show();
        });
    }

    function Delete(IssueId) {
        $.getJSON('/IssueSlip_Single/DeleteIssueDetails/', { IssueSlipId: IssueId }, function (data) {
            if (data == true) {
                alert("Deleted Successfully");
                window.location.reload();
            }
            else {
                alert("Deletion Failed");
            }
        });
    }

    function Search() {
        window.location = "/IssueSlip_Single/IssueSlipSingle?SearchFilter=" + $("#txtSearch").val();
    }
</script>

@using (Html.BeginForm("IssueSlipSingle", "IssueSlip_Single", FormMethod.Post))
{
  <div class="veh-tablecont" id="GridDetails">
            <div id="sortMsg" style="text-align: center; color: Red;"></div>
            <table class="veh-table" id="empTable">
                <thead>
                    <tr class="color-code-top-btn">
                        <td colspan="13">
