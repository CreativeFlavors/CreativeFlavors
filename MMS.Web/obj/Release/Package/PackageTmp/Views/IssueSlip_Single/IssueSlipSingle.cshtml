                        <th class="veh-7">Colour
                        </th>
                          <th class="veh-7">Required Qty
                        </th>
                        <th class="veh-7">Already Issued
                        </th>
                        <th class="veh-7">Current Issues
                        </th>

                        <th class="veh-9">Edit / Delete
                        </th>
                    </tr>
                </thead>
